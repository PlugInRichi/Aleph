        "# Convert the label ('y') to an integer instead of 'yes' or 'no'\n",
        "\n",
        "def map_label(x):\n",
        "  if x['y'] == tf.constant('yes'):\n",
        "    x['y'] = 1 \n",
        "  else :\n",
        "    x['y'] = 0\n",
        "  return x\n",
        "\n",
        "\n",
        "bank_dataset = bank_dataset.map(map_label)"
